<project name="CustomWhiteList" default="export">
    <description>
    	Custom White Listing plugin for spigot
    </description>
	<property name="cwlHome" location="${basedir}"/>

	<property name="jarPath" location="${cwlHome}\\build\\CustomWhiteList.jar"/>

    <!-- - - - - - - - - - - - - - - - - - 
          target: dep                      
         - - - - - - - - - - - - - - - - - -->
    <target name="export">
    	<echo>"Exporting to ${jarPath}"</echo>
    	<jar destfile="${jarPath}" basedir="${cwlHome}/bin" />
    </target>
	
</project>
